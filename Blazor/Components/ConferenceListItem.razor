@using RpcApi

<tr>
    <td>@Conference.Name</td>
    <td>@Conference.Location</td>
    <td>@Conference.StartDate.ToString("ddd d MMM")</td>
    @if (Conference.AttendeeTotal > 5)
    {

        <td><strong>@Conference.AttendeeTotal</strong></td>
    }
    else
    {
        <td>@Conference.AttendeeTotal</td>
    }
    <td> 
        <NavLink href="@ProposalsLink">Proposals</NavLink>
    </td>
</tr>

@code {
    [Parameter]
    public Conference Conference { get; set; }

    string ProposalsLink => $"/proposals/{Conference.Id}";
}